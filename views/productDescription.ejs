<%- include("header") -%>
    <%- include("navbar") -%>

        <div class="container mt-5">
            <div class="card">
                <div class="card-body">

                    <div class="row">
                        <div class="col-lg-5 col-md-5 col-sm-6">
                            <div class="white-box text-center">
                                <img src="/assets/images/protein-images/netura-image-1.png" class="product-image ">
                            </div>
                        </div>
                        <div class="col-lg-7 col-md-7 col-sm-6">
                            <h1 class="card-title">Netura</h1>
                            <h6 class="card-subtitle">Your Healthy Food</h6>

                            <h4 class="box-title mt-5">Product description</h4>
                            <p>Packed with richest source of nutritions (dry fruits, seeds, berries & unsweetened
                                shredded coconut). This new-age advance formulation is a love of every energy and
                                fitness enthusiast.</p>
                            <h2 class="mt-5 mb-5">
                                Rs. 1499 <small class="muted-text">(lowest price)</small>
                            </h2>
                            <form method="get" action="/product" class="d-inline">
                                <button class="btn btn-dark btn-rounded mr-1" data-toggle="tooltip" title=""
